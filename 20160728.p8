------

function lerp(a,b,x) -- interpolate
 return b*x+a*(1-x)
end

function rot(x,y,t) -- rotate
 return x*cos(t)-y*sin(t),x*sin(t)+y*cos(t)
end

------

function v_add(a,b) -- vector add
 return {
  a[1]+b[1],
  a[2]+b[2],
  a[3]+b[3]
 }
end

function v_sub(a,b) -- vector sub
 return {
  a[1]-b[1],
  a[2]-b[2],
  a[3]-b[3]
 }
end

function v_scale(v,s) -- vector scale
 return {
  v[1]*s,
  v[2]*s,
  v[3]*s
 }
end

function v_cross(a,b) -- vector cross
 return {
  a[2]*b[3]-b[2]*a[3],
  a[3]*b[1]-b[3]*a[1],
  a[1]*b[2]-b[1]*a[2]
 }
end

function v_len(v) -- vector length
 return sqrt(v[1]*v[1]+v[2]*v[2]+v[3]*v[3])
end

function v_norm(v) -- vector normalize
 local l=v_len(v)
 return {v[1]/l,v[2]/l,v[3]/l}
end

function v_ext(v) -- vector extract
 return v[1],v[2],v[3]
end

function v_prs(v) -- perspective
 local x=64+64*v[1]/max(0.01,-v[3]+5)
 local y=64+64*v[2]/max(0.01,-v[3]+5)
 return x,y
end

------

spht={} -- sphere triangle table

sphdiv=2
for iy=0,1 do
 for ix=0,2 do
  for id=0,sphdiv do
   for iv=0,id do
    local fl=1-iy*2
    
    local ym=1/(sphdiv+1)/4
    local cy1=cos(id*ym)*fl
    local cy2=cos((id+1)*ym)*fl
    local sy1=sin(id*ym)
    local sy2=sin((id+1)*ym)
    
    local xa=ix/4-iy/4
    local xm1=1/(id)/4
    local cx11=cos(iv*xm1+xa)*fl
    local cx12=cos((iv+1)*xm1+xa)*fl
    local sx11=sin(iv*xm1+xa)
    local sx12=sin((iv+1)*xm1+xa)
    
    local xm2=1/(id+1)/4
    local cx21=cos(iv*xm2+xa)*fl
    local cx22=cos((iv+1)*xm2+xa)*fl
    local sx21=sin(iv*xm2+xa)
    local sx22=sin((iv+1)*xm2+xa)
    
    add(spht,{
     {
      sy1*cx11,cy1,sy1*sx11},
     {sy2*cx21,cy2,sy2*sx21},
     {sy2*cx22,cy2,sy2*sx22},
     1-iy
    })
    
    if iv!=id then
     add(spht,{
      {sy1*cx11,cy1,sy1*sx11},
      {sy2*cx22,cy2,sy2*sx22},
      {sy1*cx12,cy1,sy1*sx12},
      1-iy
     })
    end
   end
  end
 end
end

ringt={} -- ring triangle table

local ringr=1.01
local ringth=0.04
local ringd=12
for i=1,ringd do
 local s1=sin(i/ringd)
 local s2=sin((i+1)/ringd)
 local c1=cos(i/ringd)
 local c2=cos((i+1)/ringd)
 
 add(ringt,{
  {ringr*c1,ringth,ringr*s1},
  {ringr*c1,-ringth,ringr*s1},
  {ringr*c2,-ringth,ringr*s2},
  3
 })
 add(ringt,{
  {ringr*c1,ringth,ringr*s1},
  {ringr*c2,-ringth,ringr*s2},
  {ringr*c2,ringth,ringr*s2},
  3
 })
end

buttont={}

buttonn=8
buttonr={0.26,0.15}
buttonp={0.9,0.95}
for j=1,2 do
 for i=1,buttonn do
  local s1=sin(i/buttonn)
  local s2=sin((i-1)/buttonn)
  local c1=cos(i/buttonn)
  local c2=cos((i-1)/buttonn)
  
  add(buttont,{
   {0,0,buttonp[1]},
   {c1*buttonr[j],s1*buttonr[j],buttonp[j]},
   {c2*buttonr[j],s2*buttonr[j],buttonp[j]},
   5-j*2
  })
 end
end

