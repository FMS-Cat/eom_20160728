function prepare_sprite()
 for i=1,31 do
  for j=0,11 do
   local b=1024+j*64
   memcpy(b+i*2,b,2)
  end
 end
end
prepare_sprite()

function tri(x1,y1,x2,y2,x3,y3,cb,cs)
 x1=flr(x1)
 y1=flr(y1)
 x2=flr(x2)
 y2=flr(y2)
 x3=flr(x3)
 y3=flr(y3)
 
 if y2<y1 then x1,y1,x2,y2=x2,y2,x1,y1 end
 if y3<y2 then x2,y2,x3,y3=x3,y3,x2,y2 end
 if y2<y1 then x1,y1,x2,y2=x2,y2,x1,y1 end
 if y1==y3 then return end
 
 for iy=y1,y3 do
  local xa=0
  local xb=0
  if iy<y2 then
   if y1==y2 then
    xa=x2
   else
    xa=lerp(x1,x2,(iy-y1)/(y2-y1))
   end
  else
   if y2==y3 then
    xa=x2
   else
    xa=lerp(x2,x3,(iy-y2)/(y3-y2))
   end
  end
  xb=lerp(x1,x3,(iy-y1)/(y3-y1))
  
  pal(6,sget(flr(cs),8+cb))
  pal(7,sget(flr(cs)+1,8+cb))
  local lane=sget(8+flr((cs%1)*16),iy%4)
  sspr(min(xa,xb),lane+16,abs(xb-xa)+1,1,min(xa,xb),iy)
  pal()
 end
end

function draw_bg(dep)
 cls(13)
 
 color(14)
 for i=1,16 do
  local x1,y1=v_prs({i-8.5,4,dep})
  local x2,y2=v_prs({i-8.5,-8,dep})
  line(x1,y1,x2,y2)
  
  local x1,y1=v_prs({i-8.5,4,dep})
  local x2,y2=v_prs({i-8.5,4,dep+3})
  line(x1,y1,x2,y2)
 end
 
 for i=1,13 do
  local x1,y1=v_prs({-7.5,i-9,dep})
  local x2,y2=v_prs({7.5,i-9,dep})
  line(x1,y1,x2,y2)
 end
 
 for i=1,2 do
  local x1,y1=v_prs({-7.5,4,dep+i})
  local x2,y2=v_prs({7.5,4,dep+i})
  line(x1,y1,x2,y2)
 end
 
 rectfill(0,119,128,127,12)
 
 rectfill(16,120,94,126,7)
 print("poke-8",17,121,12)
 
 rectfill(96,120,102,126,7)
 rect(97,121,100,124,12,12)
 rectfill(98,122,101,125,0)
 
 rectfill(104,120,110,126,7)
 rect(105,121,108,124,0)
 rectfill(106,122,109,125,7)
 rect(106,122,109,125,12)
end

draw_bg(-6)
memcpy(0x1000,0x6000,0x2000)

ballt=0
ballx=0
bally=-24
ballr=0

function ball_update()
 ballt+=1
 
 ballx=ballt%192
 if 96<ballx then
  ballx=192-ballx
 end
 ballx=(ballx-48)/48
 ballx*=4.5
 
 bally=(ballt+20)%64
 bally-=32
 bally=bally*bally/32/32
 bally=(bally-1)*4+1
 
 ballr=ballx/4.5*1.1
end

function draw_model(_t,_mod)
 local modr=1.0
 if _mod then modr=0.25 end

 for it=1,#_t do
  local s={}
  
  for iv=1,3 do
   local v=v_scale(_t[it][iv],3)
   
   v[3],v[1]=rot(v[3],v[1],ballr%modr)
   v[1],v[2]=rot(v[1],v[2],-0.05)
   v=v_add(v,{ballx,bally,-4})
   s[iv]={}
   s[iv][1],s[iv][2]=v_prs(v)
  end
  
  local a=v_sub(_t[it][2],_t[it][1])
  local b=v_sub(_t[it][3],_t[it][1])
  local c=v_cross(a,b)
  c=v_norm(c)
  local x,y,z=v_ext(c)
  z,x=rot(z,x,ballr%modr)
  x,y=rot(x,y,-0.05)
  
  local n=(x-y*0.6+z*2)*2.1+0.9
  
  if 0<(s[2][1]-s[1][1])*(s[3][2]-s[1][2])-(s[3][1]-s[1][1])*(s[2][2]-s[1][2]) then
   tri(s[1][1],s[1][2],s[2][1],s[2][2],s[3][1],s[3][2],_t[it][4],n)
  end
 end
end

function _draw()
 memcpy(0x6000,0x1000,0x2000)
 ball_update()
 draw_model(spht,true)
 draw_model(ringt,true)
 draw_model(buttont,false)
 
 t+=1
end
